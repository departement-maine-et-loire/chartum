<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms> 
                    <sheetsTitle>chartum backend</sheetsTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.input_2>
                        <TCEforms>
                            <label>Ajouter fichier CSV</label>
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.input_2>
                    <settings.text>
                        <TCEforms>
                            <label>Add Title</label>
                            <config>
                                <type>text</type>
                            </config>
                        </TCEforms>
                    </settings.text>
                    <settings.form>
                        <TCEforms>
                            <label>Select number</label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <items>
                                    <numIndex index="0">
                                        <numIndex index="0">Line</numIndex>
                                        <numIndex index="1">Line</numIndex>
                                    </numIndex>
                                    <numIndex index="1">
                                        <numIndex index="0">Bar</numIndex>
                                        <numIndex index="1">Bar</numIndex>
                                    </numIndex>
                                    <numIndex index="2">
                                        <numIndex index="0">Doughnut</numIndex>
                                        <numIndex index="1">Doughnut</numIndex>
                                    </numIndex>
                                    <numIndex index="3">
                                        <numIndex index="0">Pie</numIndex>
                                        <numIndex index="1">Pie</numIndex>
                                    </numIndex>
                                    <numIndex index="4">
                                        <numIndex index="0">Radar</numIndex>
                                        <numIndex index="1">Radar</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.form>
                    <settings.input>
                        <TCEforms>
                            <label>colorpicker</label>
                            <config>
                                <type>input</type>
                                <renderType>colorpicker</renderType>
                                
                            </config>
                        </TCEforms>
                    </settings.input>
                    <settings.input_3>
                        <TCEforms>
                            <label>Nom du ficher Téléchargeable</label>
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.input_3>  
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>