<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms> 
                    <sheetsTitle>chartum backend</sheetsTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.input_2>
                        <TCEforms>
                            <label>Ajouter fichier CSV</label>
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.input_2>
                    <settings.text>
                        <TCEforms>
                            <label>Ajouter un titre au graphique</label>
                            <config>
                                <type>text</type>
                            </config>
                        </TCEforms>
                    </settings.text>
                    <settings.form>
                        <TCEforms>
                            <label>Selectionner un type de graphique</label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <items>
                                    <numIndex index="0">
                                        <numIndex index="0">line</numIndex>
                                        <numIndex index="1">line</numIndex>
                                    </numIndex>
                                    <numIndex index="1">
                                        <numIndex index="0">bar</numIndex>
                                        <numIndex index="1">bar</numIndex>
                                    </numIndex>
                                    <numIndex index="2">
                                        <numIndex index="0">doughnut</numIndex>
                                        <numIndex index="1">doughnut</numIndex>
                                    </numIndex>
                                    <numIndex index="3">
                                        <numIndex index="0">pie</numIndex>
                                        <numIndex index="1">pie</numIndex>
                                    </numIndex>
                                    <numIndex index="4">
                                        <numIndex index="0">radar</numIndex>
                                        <numIndex index="1">radar</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.form>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>